{
  "name": "angular-duo-template",
  "version": "1.0.0",
  "main": "index.js",
  "author": "angular-templates.io",
  "repository": {},
  "scripts": {
    "prestart:aot:prod": "npm run build:aot:prod",
    "prestart:aot:u:prod": "npm run build:aot:u:prod",
    "prestart:jit:u:prod": "npm run build:jit:u:prod",
    "prestart:jit:prod": "npm run build:jit:prod",
    "start": "npm run start:aot:u:prod",
    "start:jit:dev": "npm run build:jit:dev",
    "start:jit:prod": "npm run serve:prod",
    "start:aot:dev": "npm run build:aot:dev",
    "start:aot:prod": "npm run serve:prod",
    "start:jit:u:dev": "npm run build:jit:u:dev",
    "start:jit:u:prod": "npm run serve:u:prod",
    "start:aot:u:dev": "npm run build:aot:u:dev",
    "start:aot:u:prod": "npm run serve:u:prod",
    "prebuild:jit:prod": "npm run CLEAN",
    "prebuild:jit:dev": "npm run CLEAN",
    "prebuild:aot:prod": "npm run CLEAN",
    "prebuild:aot:dev": "npm run CLEAN",
    "prebuild:jit:u:prod": "npm run CLEAN",
    "prebuild:jit:u:dev": "npm run CLEAN",
    "prebuild:aot:u:prod": "npm run CLEAN",
    "prebuild:aot:u:dev": "npm run CLEAN",
    "build:jit:prod": "webpack --env.browser --env.prod && webpack --env.server --env.prod",
    "build:jit:dev": "webpack-dev-server --content-base dist --progress --colors --hot --inline --history-api-fallback --port 3001 --env.browser",
    "build:aot:prod": "webpack --env.browser --env.aot --env.prod && webpack --env.server --env.aot --env.prod",
    "build:aot:dev": "webpack-dev-server --content-base dist --progress --colors --hot --inline --history-api-fallback --port 3001 --env.browser --env.aot",
    "build:jit:u:prod": "webpack --env.browser --env.prod --env.universal && webpack --env.server --env.prod --env.universal",
    "build:jit:u:dev": "webpack --env.browser --env.universal && webpack --env.server --env.universal",
    "build:aot:u:prod": "webpack --env.browser --env.aot --env.prod --env.universal && webpack --env.server --env.aot --env.prod --env.universal",
    "build:aot:u:dev": "webpack --env.browser --env.aot --env.universal && webpack --env.server --env.aot --env.universal",
    "postbuild:jit:u:dev": "npm run serve:u:dev",
    "postbuild:aot:u:dev": "npm run serve:u:dev",
    "CLEAN": "rimraf dist",
    "serve:u:dev": "nodemon dist/server.js",
    "serve:u:prod": "node dist/server.js",
    "serve:prod": "node dist/server.js",
    "serve:prod:old": "node index.js",
    "startprod": "npm run start:aot:u:prod",
    "serveprod": "node dist/server.js"
  },
  "dependencies": {
    "@angular/animations": "5.0.2",
    "@angular/cdk": "5.0.0-rc0",
    "@angular/common": "5.0.2",
    "@angular/compiler": "5.0.2",
    "@angular/core": "5.0.2",
    "@angular/forms": "5.0.2",
    "@angular/http": "5.0.2",
    "@angular/material": "5.0.0-rc0",
    "@angular/platform-browser": "5.0.2",
    "@angular/platform-browser-dynamic": "5.0.2",
    "@angular/platform-server": "5.0.2",
    "@angular/router": "5.0.2",
    "@nguniversal/express-engine": "5.0.0-beta.5",
    "bootstrap-sass": "3.3.7",
    "chart.js": "2.6.0",
    "core-js": "2.4.1",
    "d3": "3.5.17",
    "express": "4.15.3",
    "font-awesome": "4.7.0",
    "google-libphonenumber": "2.0.19",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.4.3",
    "ng2-charts": "1.6.0",
    "ng2-datepicker": "2.1.3",
    "ng2-nouislider": "1.6.1",
    "ng2-nvd3": "2.0.0",
    "ng2-smart-table": "1.2.1",
    "nouislider": "9.0.0",
    "nvd3": "1.8.5",
    "rxjs": "5.5.2",
    "xhr2": "0.1.4",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.2",
    "@angular/compiler-cli": "5.0.2",
    "@types/express": "4.0.39",
    "@types/node": "6.0.88",
    "angular-tslint-rules": "^1.0.4",
    "codelyzer": "^4.0.1",
    "concurrently": "3.4.0",
    "copy-webpack-plugin": "^4.2.0",
    "file-loader": "0.11.2",
    "html-loader": "0.5.0",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "script-ext-html-webpack-plugin": "1.8.8",
    "ts-loader": "3.1.1",
    "tslint": "^5.8.0",
    "typescript": "2.4.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-merge": "4.1.1"
  }
}
